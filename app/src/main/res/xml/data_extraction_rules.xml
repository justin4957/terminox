<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="encrypted_prefs.xml"/>
        <exclude domain="database" path="terminox.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow during device transfer but exclude keys -->
        <exclude domain="sharedpref" path="encrypted_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
